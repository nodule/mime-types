{
  "name": "extension",
  "ns": "mime",
  "description": "Get the default extension for a content-type.",
  "async": true,
  "phrases": {
    "active": "Get default content-type for {{input.in}}"
  },
  "ports": {
    "input": {
      "in": {
        "title": "Filename",
        "type": "string",
        "async": true
      }
    },
    "output": {
      "out": {
        "title": "MimeType",
        "type": "string"
      }
    }
  },
  "dependencies": {
    "npm": {
      "mime-types": "latest"
    }
  }
}
