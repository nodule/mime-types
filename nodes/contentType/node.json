{
  "name": "contentType",
  "ns": "mime",
  "description": "Create a full content-type header given a content-type or extension.",
  "async": true,
  "phrases": {
    "active": "Create content-type header for {{input.in}}"
  },
  "ports": {
    "input": {
      "in": {
        "title": "Filename",
        "type": "string",
        "async": true
      }
    },
    "output": {
      "out": {
        "title": "MimeType",
        "type": "string"
      }
    }
  },
  "dependencies": {
    "npm": {
      "mime-types": "latest"
    }
  }
}
